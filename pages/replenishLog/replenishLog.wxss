page {
  background-color: #F4F5F5;
  padding-bottom: 30rpx;
}

.replenishLog .box {
  width: 690rpx;
  margin: 30rpx auto 0;
  padding: 36rpx 32rpx;
  background: #FFFFFF;
  border-radius: 8rpx;
  box-sizing: border-box;
}

.replenishLog .box .item {
  display: flex;
}

.replenishLog .box .item+.item {
  margin-top: 16rpx;
}

.replenishLog .box .item .title {
  width: 168rpx;
  height: 40rpx;
  margin-right: 28rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #999999;
  line-height: 40rpx;
}

.replenishLog .box .item .text {
  width: 430rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
}