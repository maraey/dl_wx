page {
  background-color: #F4F5F5;
  padding-bottom: 60rpx;
}

.pileSet .box {
  width: 100vw;
  background-color: #fff;
}

.pileSet .box .item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 690rpx;
  height: 96rpx;
  margin: 0 auto;
}

.pileSet .box .item+.item {
  border-top: 2rpx solid #f5f5f5;
}

.pileSet .title {
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #333333;
  line-height: 36rpx;
}

.pileSet .box .item .switch {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 80rpx;
  height: 40rpx;
  transform: scale(.7);
}

.pileSet .box .item .item_right {
  display: flex;
  align-items: center;
}

.pileSet .box .item .item_right .input_box {
  display: flex;
  align-items: center;
  width: 156rpx;
  height: 68rpx;
  margin-right: 16rpx;
  padding: 0 16rpx;
  background: #FFFFFF;
  border-radius: 4rpx;
  border: 2rpx solid #E9EAF0;
  box-sizing: border-box;
}

.pileSet .box .item .item_right .input_box .input {
  width: 124rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #333333;
  text-align: right;
}

.pileSet .box .item .item_right .text {
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #777777;
  line-height: 36rpx;
}

.pileSet .box .item .item_right .image {
  width: 16rpx;
  height: 28rpx;
  margin-left: 24rpx;
}

.pileSet .time_box {
  width: 100vw;
  height: 388rpx;
  margin: 30rpx 0;
  padding: 30rpx;
  background: #FFFFFF;
  box-sizing: border-box;
}

.pileSet .time_box .title_box {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.pileSet .time_box .title_box .title_wrap {
  display: flex;
  align-items: center;
}

.pileSet .time_box .title_box .title_wrap .tips {
  height: 34rpx;
  margin-left: 20rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #F0513A;
  line-height: 34rpx;
}

.pileSet .time_box .title_box .title {
  height: 40rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
  line-height: 40rpx;
}

.pileSet .time_box .title_box .time_text {
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #777777;
  line-height: 36rpx;
}

.pileSet .time_box .title_box .time_selected {
  color: #3D82FF;
}

.pileSet .time_box .inputs {
  display: flex;
  justify-content: space-between;
  align-content: space-between;
  flex-wrap: wrap;
  width: 690rpx;
  height: 166rpx;
  margin-top: 24rpx;
}

.pileSet .time_box .inputs .input_box {
  display: flex;
  align-items: center;
  width: 156rpx;
  height: 68rpx;
  background: #FFFFFF;
  border-radius: 4rpx;
  border: 2rpx solid #E9EAF0;
}

.pileSet .time_box .inputs .input_box .input {
  width: 156rpx;
  padding: 0 20rpx;
  text-align: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
  box-sizing: border-box;
}

.pileSet .time_box .options {
  display: flex;
  justify-content: space-between;
  align-content: space-between;
  flex-wrap: wrap;
  width: 690rpx;
  height: 264rpx;
  margin-top: 24rpx;
}

.pileSet .time_box .options .options_item {
  width: 156rpx;
  height: 68rpx;
  line-height: 68rpx;
  text-align: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #777777;
  background: #FFFFFF;
  border-radius: 4rpx;
  border: 2rpx solid #E9EAF0;
  box-sizing: border-box;
}

.pileSet .time_box .options .options_select {
  color: #FFFFFF;
  background: #3D82FF;
  border: none;
}

.pileSet .template_box {
  width: 100vw;
  padding: 30rpx 30rpx 32rpx;
  margin-top: 30rpx;
  background-color: #fff;
  box-sizing: border-box;
}

.pileSet .template_box .tab {
  display: flex;
  flex-wrap: wrap;
  width: 690rpx;
  margin-bottom: 40rpx;
}

.pileSet .template_box .tab .tab_item {
  width: 140rpx;
  height: 64rpx;
  margin-top: 24rpx;
  line-height: 64rpx;
  text-align: center;
  font-size: 26rpx;
  font-weight: 400;
  color: #AAAAAA;
  background: #F4F5F5;
  border-radius: 33rpx;
}

.pileSet .template_box .tab .tab_item+.tab_item {
  margin-left: 42rpx;
}

.pileSet .template_box .tab .tab_select {
  color: #FFFFFF;
  background: #3D82FF;
  box-shadow: 0rpx 4rpx 8rpx 0rpx rgba(61, 130, 255, 0.2);
  border-radius: 32rpx;
}

.pileSet .template_box .fastSet {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 690rpx;
  margin-bottom: 30rpx;
}

.pileSet .template_box .fastSet .item_right {
  display: flex;
  align-items: center;
}

.pileSet .template_box .fastSet .item_right .input_box {
  display: flex;
  align-items: center;
  width: 156rpx;
  height: 68rpx;
  margin-right: 16rpx;
  padding: 0 16rpx;
  background: #FFFFFF;
  border-radius: 4rpx;
  border: 2rpx solid #E9EAF0;
  box-sizing: border-box;
}

.pileSet .template_box .fastSet .item_right .input_box .input {
  width: 124rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #333333;
  text-align: right;
}

.pileSet .template_box .fastSet .item_right .text {
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #777777;
  line-height: 36rpx;
}

.pileSet .template_box .table {
  margin-top: 24rpx;
  border-top: 2rpx solid #E9EAF0;
  border-left: 2rpx solid #E9EAF0;
}

.pileSet .template_box .list_list {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 230rpx;
  height: 80rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #333333;
  border-right: 2rpx solid #E9EAF0;
  border-bottom: 2rpx solid #E9EAF0;
  box-sizing: border-box;
}

.pileSet .template_box .list_list .input {
  width: 180rpx;
  text-align: center;
}

.pileSet .template_box .list_list .power {
  width: 60rpx;
}

.pileSet .template_box .list_title_box {
  display: flex;
  width: 690rpx;
  height: 80rpx;
}

.pileSet .template_box .list_title_box .list_list {
  font-size: 26rpx;
  font-weight: 500;
}

.pileSet .template_box .table_item {
  display: flex;
  width: 690rpx;
}

.pileSet .template_box .colorGrey {
  background-color: #F8F8F8;
}

.pileSet .template_box .colorBlue {
  background-color: #F0F8FE;
}

.pileSet .operate {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  width: 690rpx;
  height: 80rpx;
  margin: 60rpx auto 75rpx;
}

.pileSet .operate .btn {
  width: 160rpx;
  height: 80rpx;
  margin-bottom: 20rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 26rpx;
  font-weight: 400;
  color: #3D82FF;
  background: #FFFFFF;
  border-radius: 4rpx;
  border: 2rpx solid #3D82FF;
  box-sizing: border-box;
}

.pileSet .operate .confirm {
  width: 328rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 26rpx;
  font-weight: 500;
  color: #FFFFFF;
  background: #3D82FF;
  border-radius: 4rpx;
}
.pileSet .operate .widthAll{
  width: 690rpx;
}
.popup {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.3);
  z-index: 2;
}

.popup .popup_box {
  position: absolute;
  top: 340rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 622rpx;
  height: 310rpx;
  background: #FFFFFF;
  border-radius: 8rpx;
  z-index: 3;
}

.popup .popup_box .popup_title {
  height: 42rpx;
  margin: 30rpx 0 28rpx;
  text-align: center;
  font-size: 30rpx;
  font-weight: 500;
  color: #333333;
  line-height: 42rpx;
}

.popup .popup_box .popup_input_box {
  display: flex;
  align-items: center;
  width: 534rpx;
  height: 82rpx;
  padding: 0 24rpx;
  margin: 0 auto;
  background: #F8F8F8;
  border-radius: 4rpx;
  box-sizing: border-box;
}

.popup .popup_box .popup_input_box .input {
  width: 486rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #000000;
}

.popup .popup_box .popup_operate {
  display: flex;
  align-items: center;
  margin-top: 30rpx;
  border-top: 2rpx solid #F5F5F5;
}

.popup .popup_box .popup_operate .popup_btn {
  width: 310rpx;
  height: 96rpx;
  line-height: 96rpx;
  text-align: center;
  font-size: 30rpx;
  font-weight: 400;
  color: #333333;
}

.popup .popup_box .popup_operate .confirm {
  font-size: 30rpx;
  font-weight: 500;
  color: #3D82FF;
}

.popup .popup_box .popup_operate .line {
  width: 2rpx;
  height: 26rpx;
  background: #F5F5F5;
}

.popup .popup_box .popup_item {
  display: flex;
  justify-content: space-between;
  width: 518rpx;
  margin: 0 auto;
  padding-top: 24rpx;
}

.popup .popup_box .popup_item+.popup_item {
  margin-top: 14rpx;
  border-top: 2rpx solid #f5f5f5;
}

.popup .popup_box .popup_item .popup_item_title {
  height: 40rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
  line-height: 40rpx;
}

.popup .popup_box .popup_item .radio {
  display: flex;
  align-items: center;
  transform: scale(.75);
}