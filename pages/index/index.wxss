.index{
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 30rpx 30rpx 0 30rpx;
}
.index .profit{
  display: flex;
  flex-direction: column;
  width: 690rpx;
  padding: 32rpx 32rpx 26rpx 32rpx;
  background: linear-gradient(135deg, #3D82FF 0%, #6EB7FF 100%);
  border-radius: 8rpx;
  box-sizing: border-box;
}
.index .profit .profit_title{
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, .5);
  line-height: 40rpx;
}
.index .profit .profit_number_big{
  margin: 12rpx 0 16rpx 0;
  font-size: 64rpx;
  font-weight: 500;
  color: #ffffff;
  line-height: 78rpx;
}
.index .profit .profit_middle_wrap{
  display: flex;
  flex-flow: wrap;
  justify-content: space-between;
}
.index .profit .profit_middle_wrap .warp{
  display: flex;
  width: 300rpx;

}
.index .profit .profit_middle_wrap .profit_title{
  margin-right: 16rpx;
}
.index .profit .profit_number{
  font-size: 30rpx;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 36rpx;
}
.index .profit .profit_number_small{
  font-size: 30rpx;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 36rpx;
}
.index .profit .profit_line{
  width: 626rpx;
  margin: 24rpx 0 16rpx 0 ;
  border-top: 1rpx solid rgba(255, 255, 255, .2);
}
.index .profit .profit_bottom_wrap{
  display: flex;
  flex-flow: wrap;
  justify-content: space-between;
}
.index .profit .profit_bottom_wrap .profit_bottom_wrap_title{
  display: flex;
  width: 690rpx;
  justify-content: space-between;
}
.index .profit .profit_bottom_wrap .profit_bottom_wrap_title .profit_wrap{
  display: flex;
  flex-direction: column;
}
.index .profit .profit_bottom_wrap .profit_bottom_wrap_title .profit_wrap_s{
  display: flex;
  flex-direction: column;
  margin-left: -30rpx;
}
.index .profit .profit_bottom_wrap .profit_bottom_wrap_title .profit_wrap .profit_title{
  margin-bottom: 4rpx;
}
.index .middle{
  display: flex;
  justify-content: space-between;
  width: 690rpx;
  margin: 30rpx 0 40rpx 0;
}
.index .middle .image{
  width: 328rpx;
  height: 120rpx;
  box-shadow: 0 4rpx 12rpx rgba(61, 130, 255, .1);
}
.index .lists{
  display: flex;
  flex-wrap: wrap;
  margin-top: 40rpx;
  width: 690rpx;
}
.index .lists .item{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 25%;
  margin-bottom: 40rpx;
}
.index .lists .item .image{
  width: 56rpx;
  height: 56rpx;
  margin-bottom: 16rpx;
}
.index .lists .item .images{
  width: 80rpx;
  height: 80rpx;
  margin-bottom: 16rpx;
}
.index .lists .item .text{
  font-size: 26rpx;
  font-weight: 400;
  color: #333333;
  line-height: 36rpx;
}
.entrance{
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 30rpx;
}
.entrance .box{
  display: flex;
  flex-direction: column;
  width: 690rpx;
  height: 364rpx;
  padding: 40rpx 32rpx;
  background: linear-gradient(135deg, #3D82FF 0%, #6EB7FF 100%);
  border-radius: 8rpx;
  box-sizing: border-box;
}
.entrance .box .title{
  margin-bottom: 12rpx;
  font-size: 26rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, .5);
  line-height: 36rpx;
}
.entrance .box .acount{
  font-size: 64rpx;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 78rpx;
}
.entrance .quick_entrance{
  display: flex;
  justify-content: space-between;
  width: 690rpx;
  margin-top: 30rpx;
}
.entrance .quick_entrance .image{
  width: 332rpx;
  height: 128rpx;
  box-shadow: 0 4rpx 12rpx rgba(61, 130, 255, .1);
}

.tabbar {
  display: flex;
  width: 690rpx;
  margin-bottom: 30rpx;
}

.tabbar .tabbar_item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.tabbar .tabbar_item+.tabbar_item {
  margin-left: 40rpx;
}

.tabbar .tabbar_item .title {
  height: 40rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
  line-height: 40rpx;
}

.tabbar .tabbar_item .line {
  width: 48rpx;
  height: 8rpx;
  margin-top: 10rpx;
}

.tabbar .selected .title {
  height: 44rpx;
  font-size: 32rpx;
  font-weight: 500;
  color: #3D82FF;
  line-height: 44rpx;
}

.tabbar .selected .line {
  width: 48rpx;
  height: 8rpx;
  margin-top: 8rpx;
  background: #3D82FF;
  border-radius: 4rpx;
}
.index .profit .profit_title_title {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}

.index .profit .profit_title_title .profit_title_text {
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, .5);
  line-height: 40rpx;
}

.index .profit .profit_title_title .profit_title_time {
  height: 32rpx;
  font-size: 22rpx;
  font-weight: 400;
  color: #FFFFFF;
  line-height: 32rpx;
}
.welcome{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100vw;
  height: 100vh;
}

.welcome .wel_img{
  margin-top: 180rpx;
  margin-bottom: 52rpx;
  width: 438rpx;
  height: 450rpx;
}

.welcome .tittle{
  margin-bottom: 40rpx;
  height: 50rpx;
  font-size: 36rpx;
  font-weight: 500;
  color: #2F3B4E;
  line-height: 42rpx;
}

.welcome .btn{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 240rpx;
  height: 80rpx;
  background: linear-gradient(135deg, #3D82FF 50%, #47ACFC 100%);
	border-radius: 40rpx;
	box-sizing: border-box;
}

.welcome .btn .btn_text{
  width: 104rpx;
  height: 36rpx;
  font-size: 26rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 30rpx;
}