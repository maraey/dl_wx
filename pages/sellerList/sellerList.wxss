page {
  background: #F4F5F5;
}

.agencyList {
  width: 100vw;
  padding: 30rpx 0;
  box-sizing: border-box;
}

.agencyList .input_box {
  display: flex;
  align-items: center;
  width: 690rpx;
  height: 72rpx;
  margin: 0 auto;
  padding: 0 30rpx;
  font-size: 22rpx;
  background: #ffffff;
  border-radius: 4rpx;
  box-sizing: border-box;
}

.agencyList .input_box .image {
  width: 28rpx;
  height: 28rpx;
  margin-right: 22rpx;
}

.agencyList .input_box .input {
  width: 570rpx;
}

.agencyList .box {
  width: 690rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  padding: 36rpx 32rpx 24rpx;
  background: #FFFFFF;
  border-radius: 8rpx;
  box-sizing: border-box;
}

.agencyList .box .name {
  height: 44rpx;
  margin-bottom: 24rpx;
  font-size: 32rpx;
  font-weight: 500;
  color: #333333;
  line-height: 44rpx;
}

.agencyList .box .bottom {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.agencyList .box .bottom .detail .image{
  width: 12rpx;
  height: 24rpx;
}
.agencyList .box .bottom .info .phone {
  display: flex;
  align-items: center;
  height: 40rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #999999;
  line-height: 40rpx;
}
.agencyList .box .bottom .info .phone+.phone{
  margin-top: 16rpx;
}
.agencyList .box .bottom .info .phone .phone_title {
  height: 40rpx;
  margin-right: 28rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: #999999;
  line-height: 40rpx;
}