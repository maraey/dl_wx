.agencyDetail .top_box {
  width: 750rpx;
  background: #FFFFFF;
}

.agencyDetail .top_box .top {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 54rpx 24rpx 30rpx;
  box-sizing: border-box;
}

.agencyDetail .top_box .top .title_and_rate .title {
  height: 44rpx;
  margin-bottom: 12rpx;
  font-size: 32rpx;
  font-weight: 500;
  color: #000000;
  line-height: 44rpx;
}

.agencyDetail .top_box .top .title_and_rate .rate {
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #999999;
  line-height: 36rpx;
}

.agencyDetail .top_box .top .title_and_rate .rate .rate_text {
  margin-right: 28rpx;
}

.agencyDetail .top_box .top .delete {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.agencyDetail .top_box .top .delete .image {
  width: 40rpx;
  height: 40rpx;
  margin-bottom: 12rpx;
}

.agencyDetail .top_box .top .delete .delete_text {
  height: 32rpx;
  font-size: 22rpx;
  font-weight: 400;
  color: #333333;
  line-height: 32rpx;
}

.agencyDetail .top_box .line {
  width: 100vw;
  border-top: 2rpx solid #f5f5f5;
}

.agencyDetail .top_box .middle {
  padding: 30rpx;
  box-sizing: border-box;
}

.agencyDetail .top_box .middle .item {
  display: flex;
  align-items: center;
}

.agencyDetail .top_box .middle .item+.item {
  margin-top: 16rpx;
}

.agencyDetail .top_box .middle .item .item_title {
  width: 156rpx;
  height: 36rpx;
  margin-right: 64rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #999999;
  line-height: 36rpx;
}

.agencyDetail .top_box .middle .item .item_text {
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #333333;
  line-height: 36rpx;
}

.agencyDetail .top_box .bottom {
  display: flex;
  align-items: center;
  padding: 24rpx 46rpx 36rpx;
  box-sizing: border-box;
}

.agencyDetail .top_box .bottom .item {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.agencyDetail .top_box .bottom .item+.item {
  margin-left: 90rpx;
}

.agencyDetail .top_box .bottom .item .image {
  width: 80rpx;
  height: 80rpx;
  margin-bottom: 16rpx;
}

.agencyDetail .top_box .bottom .item .text {
  height: 34rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #333333;
  line-height: 34rpx;
}

.agencyDetail .line_bottom {
  width: 100vw;
  height: 20rpx;
  margin-bottom: 36rpx;
  background-color: #F4F5F5;
}

.agencyDetail .bottom_box .device_battery {
  width: 690rpx;
  height: 262rpx;
  margin: 0 auto;
  margin-bottom: 30rpx;
  border-radius: 8rpx;
  border: 2rpx solid #EEEEEE;
  box-sizing: border-box;
}

.agencyDetail .bottom_box .device_battery .title {
  height: 88rpx;
  padding-left: 32rpx;
  line-height: 88rpx;
  border-bottom: 2rpx solid #F5F5F5;
}

.agencyDetail .bottom_box .device_battery .list {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 172rpx;
  padding: 0 32rpx;
  box-sizing: border-box;
}

.agencyDetail .bottom_box .device_battery .list .item {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.agencyDetail .bottom_box .device_battery .list .item .item_title {
  height: 36rpx;
  margin-bottom: 20rpx;
  font-size: 26rpx;
  font-weight: 500;
  color: #999999;
  line-height: 36rpx;
}

.agencyDetail .bottom_box .device_battery .list .item .item_num {
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 500;
  color: #3D82FF;
  line-height: 36rpx;
}