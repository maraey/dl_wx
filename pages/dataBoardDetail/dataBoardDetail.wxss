page {
  background: #F4F5F5;
}

.box {
  width: 690rpx;
  margin: 18rpx auto;
  padding: 30rpx 0 40rpx;
  background: #FFFFFF;
  border-radius: 8rpx;
  box-sizing: border-box;
}

.box .title_box {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 630rpx;
  margin: 0 auto;
}

.box .title_box .title {
  height: 40rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: #333333;
  line-height: 40rpx;
}

.box .line {
  width: 630rpx;
  height: 2rpx;
  margin: 30rpx auto;
  background: #F5F5F5;
}

.box .info_box {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 640rpx;
  margin: 0 auto;
}

.box .info_box .text {
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #333333;
  line-height: 36rpx;
}

.box .info_box .name_box {
  display: flex;
  align-items: center;
  width: 190rpx;
}

.box .info_box .name_box .image {
  width: 24rpx;
  height: 12rpx;
  margin-left: 12rpx;
}

.w134 {
  width: 134rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.w170 {
  width: 130rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.w180 {
  width: 180rpx;
  text-align: center;
}
.w150 {
  width: 180rpx;
  text-align: center;
}

.w100 {
  width: 100rpx;
  text-align: center;
}

.w60 {
  width: 60rpx;
}

.box .row{
  margin: 30rpx 0;
}
/* .box .row+.row {
  margin-top: 30rpx;
} */

.box .info_box .name_box .child {
  padding-left: 20rpx;
}
.box .info_box .w60{
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #999;
  line-height: 36rpx;
}
.box .info_box .blue {
  height: 36rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #3D82FF;
  line-height: 36rpx;
}